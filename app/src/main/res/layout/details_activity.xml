<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
    <TextView android:text="Device ID:" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/device_id" android:layout_marginTop="5dp"></TextView>
    <TextView android:text="Device IP:" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/device_ip" android:layout_marginTop="5dp"></TextView>
    <TextView android:text="Tuner:" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/tuner_num" android:layout_marginTop="5dp"></TextView>
    <TextView android:text="Device Model:" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/device_model" android:layout_marginTop="5dp"></TextView>
    <TextView android:text="Firmware:" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/firmware_version" android:layout_marginTop="5dp"></TextView>
    <TextView android:text="Lockkey Owner:" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/lockkey_owner" android:layout_marginTop="5dp"></TextView>
    <TextView android:text="Target_IP:" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/target_ip" android:layout_marginTop="5dp"></TextView>
    
</LinearLayout>
